<div id="user_header">  
  <% if current_user %>  
    Logged in as <%= current_user.email %>.  
    <%= link_to "Log Out", logout_path %>  
  <% else %>  
    <%= link_to "Sign Up", signup_path %> or  
    <%= link_to "Log In", login_path %>  
  <% end %>  
</div>  

  <%= content_tag(:div, class: "alert alert-#{:login_error}") do %>  
   <p><%= flash[:login_error] %></p>  
  <% end %>  
